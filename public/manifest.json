{
  "manifest_version": 3,
  "name": "Chrome Extension Starter Kit",
  "version": "0.1.0",
  "description": "",
  "icons": {
    "48": "icons/icon.png"
  },
  "permissions": ["storage", "activeTab"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["content.js"]
    }
  ]
}
